<scroll-view scroll-y class="scrollPage">

  <view class="UCenter-bg">
    <view class="userinfo">
      <view class="userinfo-avatar">
        <open-data type="userAvatarUrl"></open-data>
      </view>
      <open-data class="name" type="userNickName"></open-data>
    </view>
    <image src="https://raw.githubusercontent.com/weilanwl/ColorUI/master/demo/images/wave.gif" mode="scaleToFill" class="gif-wave"></image>
  </view>

  <view class="padding flex text-center text-grey bg-white shadow-warp">

    <view class="flex flex-sub flex-direction solid-right">
      <view class="text-xxl text-black">{{count}}</view>
      <view class="margin-top-sm">
        已转化图片数
      </view>
    </view>

  </view>

  <view class="cu-list menu card-menu margin-top-xl shadow-lg radius">
    <view class="cu-item arrow">
      <navigator class="content" url="/pages/history/history" hover-class="none">
        <text class="cuIcon-time text-geryk"></text>
        <text class="text-back">历史记录</text>
      </navigator>
    </view>
  </view>

  <view class="cu-list menu card-menu shadow-lg radius">

    <view class="cu-item arrow">
      <navigator class="content" url="/pages/feedback/feedback" hover-class="none">
        <text class="cuIcon-writefill text-grey"></text>
        <text class="text-black">意见反馈</text>
      </navigator>
    </view>
  </view>

  <view class="cu-list menu card-menu margin-bottom-xl shadow-lg radius">
    <view class="cu-item arrow">
      <navigator class="content" url="/pages/about/about" hover-class="none">
        <text class="cuIcon-news text-grey"></text>
        <text class="text-black">关于项目</text>
      </navigator>
    </view>
  </view>

  <view class="cu-tabbar-height"></view>
</scroll-view>